{{ if isEndpoint .Node }}
{{ info "### generic_cc ###" }}

{{ range $cc := jsonUnmarshal .Node.D.cc_commands }}
	cc filter uuid={{ $.Node.D.uuid }}
	{{ if $cc.send }}
		{{ range $v := $cc.send }}
			cc send {{ $v }}
		{{ end }}
	{{ else if $cc.recv }}
		{{ range $v := $cc.recv }}
			cc recv {{ $v }}
		{{ end }}
	{{ else if $cc.exec }}
		{{ range $v := $cc.exec }}
			cc exec {{ $v }}
		{{ end }}
	{{ else if $cc.background }}
		{{ range $v := $cc.background }}
			cc background {{ $v }}
		{{ end }}
	{{ else }}
		{{ error "invalid cc command: %v" $cc }}
	{{ end }}
	clear cc filter
{{ end }}

{{ end }}
